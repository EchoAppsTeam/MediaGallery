{
    "id": "{%=manifest.id%}",
    "name": "{%=manifest.name%}",
    "description": "MediaGallery application allows you to configure multiple data sources and visualize them on a page",
    "app_version": "{%=packageVersions.stable%}",
    "icon": "{%=baseURLs.prod%}/images/icon.png",
    "developer": {
        "id": "echo",
        "name": "Echo",
        "url": "http://www.aboutecho.com",
        "email": "solutions@aboutecho.com",
        "icon": "http://cdn.echoenabled.com/images/echo.png"
    },
    "dashboards": [{
        "id": "instances",
        "type": "instances",
        "title": "Instances",
        "script": "{%=baseURLs.prod%}/dashboard.js",
        "component": "Echo.Apps.MediaGallery.Dashboard",
        "config": {
            "instanceManager": {
                "instance": {"useLoadingIcon": true},
                "preview": {
                    "dimensions": {
                        "width": 60,
                        "maxWidth": 70,
                        "minWidth": 40
                    }
                }
            },
            "appkeys": "{data:apps.streamserver.appkeys}",
            "janrainapps": "{data:apps.janrain.apps}",
            "customer": "{data:customer}",
            "domains": "{data:apps.streamserver.domains}"
        }
    }, {
      "id": "docs",
      "type": "external",
      "title": "Documentation",
      "config": {
        "src": "http://emb.echoplatform.com/mediagallery/docs/overview/"
      }
    }],
    "client_widget": {
        "component": "Echo.Apps.MediaGallery",
        "scripts": {
          "dev": "{%=baseURLs.dev%}/app.js",
          "prod": "{%=baseURLs.prod%}/app.js"
        }
    },
    "cloudapp_protocol_version": "1.0.0"
}
